#. module: mass_mailing_opt_in
#: model:mail.template,body_html:mass_mailing_opt_in.mass_mailing_opt_in_welcome_email_template
msgid ""
"\n"
"          \n"
"          <h1>Hello ${object.name}.</h1>\n"
"          <p>you are almost ready to get the letter. Just click on the link below:</p>\n"
"          <p><a href=\"${object.subscribe_to_newsletter_url}\">Subscribe</a></p>\n"
"          <p>Thank you.</p>\n"
"          \n"
"        "
msgstr ""
"\n"
"          \n"
"          <h1>Hallo ${object.name}.</h1>\n"
"          <p>Sie haben es fast geschafft.  Bitte klicken Sie einfach auf diese Link um ihre Emailadresse zu bestätigen.</p>\n"
"          <p><a href=\"${object.subscribe_to_newsletter_url}\">Anmelden</a></p>\n"
"          <p>Herzlichen Dank!</p>\n"
"          \n"
"        "

#. module: mass_mailing_opt_in
#: model:ir.ui.view,arch_db:mass_mailing_opt_in.contact_not_found
msgid ""
"<b>Sorry,</b><br/>\n"
"                  We could not find any contact to validate on this location."
msgstr ""
"<b>Es tut uns leid,</b><br/>\n"
"                  Wir konnten unter diesem Link keinen Eintrag finden."

